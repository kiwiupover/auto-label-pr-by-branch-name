{
  "name": "auto-label-pr-based-on-pr-branch-name",
  "private": true,
  "description": "A typescript template for rapid development of GitHub actions.",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiwiupover/action-ts-template",
    "branch": "main"
  },
  "license": "MIT",
  "main": "src/main.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "package": "ncc build src/main.ts -o dist",
    "all": "npm run format-check && npm run lint && npm run package"
  },
  "dependencies": {
    "@actions/core": "^1.8.2",
    "@actions/github": "^5.0.3",
    "@octokit/rest": "^18.12.0",
    "actions-util": "^1.1.4"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.27.0",
    "@umijs/fabric": "^2.10.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.16.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.7.2"
  }
}
